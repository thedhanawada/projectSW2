<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:type="radial"
                android:centerX="50%"
                android:centerY="50%"
                android:startColor="#FFFFFF00"
                android:endColor="#FF008080"
                android:gradientRadius="90"/>
            <gradient
                android:type="linear"
                android:startColor="#FFFFFF00"
                android:endColor="#FF008080"
                android:angle="45"/>
        </shape>
    </item>
</selector>
